{
  "_version": "1.21.0",
  "sap.app": {
    "id": "com.appIdeas.appIdeas",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "foodCategoriesRemote": {
      "uri": "https://www.themealdb.com/api/json/v1/1/list.php?c=list",
      "type": "JSON"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.appIdeas.appIdeas.view.App",
      "type": "XML",
      "async": true,
      "id": "idAppControl"
    },
    "dependencies": {
      "minUI5Version": "1.60.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.appIdeas.appIdeas.i18n.i18n"
        }
      },
      "foodCategories": {
        "dataSource": "foodCategoriesRemote"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.appIdeas.appIdeas.view",
        "controlId": "idAppControl",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "overview",
          "target": "overview"
        },
        {
          "pattern": "bin2dec",
          "name": "bin2dec",
          "target": "bin2dec"
        },
        {
          "pattern": "recipebox",
          "name": "recipebox",
          "target": "recipebox"
        },
        {
          "pattern": "recipebox/{foodCategory}",
          "name": "foodcategory",
          "target": "foodcategory"
        },
        {
          "pattern": "weatherapp",
          "name": "weatherapp",
          "target": "weatherapp"
        },
        {
          "pattern": "wordfrequency",
          "name": "wordfrequency",
          "target": "wordfrequency"
        }
        ],
        "targets": {
          "overview": {
            "viewName": "Overview"
          },
          "bin2dec": {
            "viewName": "Bin2dec"
          },
          "recipebox": {
            "viewName": "Recipebox"
          },
          "foodcategory":{
            "viewName": "Foodsbycategory"
          },
          "weatherapp":{
            "viewName": "Weatherapp"
          },
          "wordfrequency": {
            "viewName": "Wordfrequency"
          }
        }
    }
  }
}
